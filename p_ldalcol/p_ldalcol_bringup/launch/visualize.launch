<launch>
    <!--Bloco 1: argumentos do launch file.-->
    <arg name="player_name" default="p_ldalcol"/>

    <!-- Launch rviz, loading configuration file -->
    <node name="rviz" pkg="rviz" type="rviz" args="--display-config $(find p_ldalcol_bringup)/config/$(arg player_name).rviz" required="false"/>
</launch>